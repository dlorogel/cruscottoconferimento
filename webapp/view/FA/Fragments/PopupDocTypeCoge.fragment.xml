		</content>
		<buttons>
			<Button
				text="{i18n>lbl.Conferma}"
				type="Emphasized"
				press="onConfermaCoge"
			/>
			<Button
				text="{i18n>lbl.Annulla}"
				type="Reject"
				press="onAnnullaCoge"
			/>
		</buttons>
