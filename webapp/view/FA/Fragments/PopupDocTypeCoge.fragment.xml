<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:o="sap.ui.layout.form"
	controllerName="it.orogel.cruscottoconferimento.controller.FA.FA"
>
	<Dialog
		title="{i18n>lbl.InserireValori}"
		contentHeight="20%"
		contentWidth="60%"
	>
		<content>
			<o:SimpleForm
				editable="true"
				layout="ColumnLayout"
				singleContainerFullSize="false"
				columnsXL="2"
				columnsL="2"
				columnsM="2"
				visible="true"
			>
				<Label
                    required="true"
					text="Tipo Documento"
					labelFor="Note"
					class="sapUiSmallMarginTop"
				/>
				<Input
					id="iTipoDocumentoM3"
					type="Text"
					value="{emissioneFatturaModel>/TipoDocumento}"
					description="{appModel>/TipoDocumentoDescription}"
					showValueHelp="true"
					valueHelpOnly="false"
					valueHelpRequest=".onValueHelpCoge($event,'TipoDocumentoSet')"
					suggest=".onValueHelpCoge($event,'TipoDocumentoSet')"
				/>
				<Label
                    required="true"
					text="Conto CoGe"
					labelFor="iNote1"
					class="sapUiSmallMarginTop"
				/>
				<Input
					id="iContiCoGeM3"
					type="Text"
					value="{emissioneFatturaModel>/ContoCoge}"
					description="{appModel>/ContoCogeDescription}"
					showValueHelp="true"
					valueHelpOnly="false"
					valueHelpRequest=".onValueHelpCoge($event,'ContiCoGeSet')"
					suggest=".onValueHelpCoge($event,'ContiCoGeSet')"
				/>
			</o:SimpleForm>
